<% form_tag(session_url(:protocol => protocol)) do %>
  <p><label for="login">Nom d'utilisateur:</label><br /><%= text_field_tag :login, "" %></p>
  <p><label for="password">Mot de passe:</label><br /><%= password_field_tag :password, "" %></p>
  <p><%= submit_tag "Ouvrir une session" %></p>
<% end %>
